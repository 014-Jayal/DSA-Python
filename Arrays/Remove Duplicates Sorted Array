class Solution:
    def removeDuplicates(self, arr):
        
        seen = set(arr)
        org = list(seen)
        org.sort()
        return org

----------------------------------------------

class Solution:
    def removeDuplicates(self, arr):
        b = []
        for i in range(len(arr) - 1):
            if arr[i] != arr[i + 1]:
                b.append(arr[i])
        b.append(arr[-1])
        return b
