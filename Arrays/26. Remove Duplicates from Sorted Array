class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        
        n = len(nums)
        seen = set()
        idx = 0

        for i in range(n):
            if nums[i] not in seen:
                seen.add(nums[i])
                nums[idx] = nums[i]
                idx += 1

        return idx 

------------------------------------------------------------------

