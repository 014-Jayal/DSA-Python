class Solution:
    def maxFrequencyElements(self, nums: List[int]) -> int:

        n = len(nums)
        hashd = dict()

        for x in nums:
            hashd[x] = hashd.get(x, 0) + 1

        mfreq = max(hashd.values())

        count = 0

        for key, value in hashd.items():
            if hashd[key] == mfreq:
                count += value

        return count

---------------------------------------------------------------------

from collections import counter

class Solution:
    def maxFrequencyElements(self, nums: List[int]) -> int:
        freq = counter(nums)
        mfreq = max(freq.values())

        return sum(v for v in freq.values() if v == mfreq)

----------------------------------------------------------------------

class Solution:
    def maxFrequencyElements(self, nums: List[int]) -> int:
        c=[]
        for i in set(nums):
            c.append(nums.count(i))
        return (max(c)*c.count(max(c)))
