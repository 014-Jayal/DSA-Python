#Using loops

class Solution:
    def findFrequency(self, arr, x):
        
        count = 0
        for item in arr:
            if x == item:
                count +=1
                
        return count

----------------------------------------------

#Using Filter() function

class Solution:
    def findFrequency(self, arr, x):
        
        itr = filter(lambda n: n == x, arr)
        
        res = len(list(itr))
        
        return res

